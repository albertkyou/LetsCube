{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-param-reassign": [
            "error",
            {
                "props": false
            }
        ],
        "no-console": 2,
        "no-underscore-dangle": 0,
        "func-names": 0,
        "consistent-return": 0
    }
}